async function validateRollback() {
  try {
    console.log('Testing rollback procedures...');

    // Test git rollback
    const result = spawn('git', ['checkout', 'rollback-phase1-start'], { stdio: 'pipe' });
    await new Promise((resolve) => result.on('close', resolve));

